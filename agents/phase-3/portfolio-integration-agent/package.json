{
  "name": "@clearway/portfolio-integration-agent",
  "version": "1.0.0",
  "description": "Multi-platform portfolio data integration for Clearway Phase 3",
  "main": "backend/index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only backend/index.ts",
    "build": "tsc",
    "start": "node dist/backend/index.js",
    "migrate": "node scripts/migrate.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "prom-client": "^15.0.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/jest": "^29.5.8",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0",
    "nock": "^13.4.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clearway/portfolio-integration-agent"
  },
  "keywords": [
    "portfolio",
    "integration",
    "black-diamond",
    "orion",
    "addepar",
    "wealth-management"
  ],
  "author": "Clearway Team",
  "license": "PROPRIETARY"
}
